{
  "name": "flux-shopping-cart",
  "version": "0.0.0",
  "description": "A Flux app that allows users to manage a shopping cart",
  "author": "Graham Swan <thinkswan@gmail.com> (http://grahamswan.com/)",
  "scripts": {
    "browserify": "browserify app.js | uglifyjs -cm > public/js/app.js",
    "build": "npm run browserify ",
    "watch": "watchify app.js -o public/js/app.js -v",
    "postinstall": "npm run build",
    "start": "node server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/thinkswan/flux-shopping-cart.git"
  },
   "bugs": {
    "url": "https://github.com/thinkswan/flux-shopping-cart/issues"
  },
  "keywords": [
    "node",
    "express",
    "flux",
    "react",
    "underscore",
    "e-commerce"
  ],
  "dependencies": {
    "flux": "^2.0.3",
    "react": "^0.13.3",
    "express": "^4.12.4",
    "underscore": "^1.8.3",
    "browserify": "~10.2.4",
    "reactify": "^1.1.1",
    "envify": "^3.4.0",
    "uglify-js": "^2.4.23"
  },
  "devDependencies": {
    "watchify": "^3.2.2"
  },
  "engines": {
    "node": "^0.12.4"
  },
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  },
  "private": true,
  "license": "MIT"
}
